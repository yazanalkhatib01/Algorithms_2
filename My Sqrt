#include <iostream>
using namespace std;

/* My Sqrt
Problem #50 */

float ReadNumber() {

	float Number;
	cout << "Enter Number: ";
	cin >> Number;

	return Number;
}

float MySqrt(float Number) {

	return pow(Number, 0.5);
}

int main() {
	
	float Number = ReadNumber();

	cout << "\nMy Round Result: " << MySqrt(Number);
	cout << "\nC++ Round Result: " << sqrt(Number) << endl;

	return 0;
}
